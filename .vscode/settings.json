{
  "deno.suggest.imports.hosts": {
    "https://deno.land": true
  },
  "editor.tabSize": 2,
  "deno.lint": true,
  "deno.enable": true,
  "deno.unstable": true,
  "deno.importMap": "./dev_import_map.json",
  "editor.defaultFormatter": null,
  "deno.enablePaths": [
    "./game-server",
    "./http",
    "./scripts",
    "./types",
    "./config.json",
    "./main.ts"
  ],
  "volar.completion.preferredAttrNameCase": "camel",
  "volar.completion.preferredTagNameCase": "pascal",
  "volar.takeOverMode.enabled": "auto",
  "volar.diagnostics.delay": 100
}
